{
  "name": "@makepad/growthing",
  "description": "An open-source full stack growth tool",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "core/*",
    "scrapers/*",
    "commons/*",
    "automations/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@makepad/eslint-config": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.26.1"
}
